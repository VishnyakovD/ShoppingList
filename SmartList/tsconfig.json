{
	"version": "2.6.2",
	"compileOnSave": true,
	"compilerOptions": {
		"target": "es5",
		"module": "amd",
		"noImplicitAny": false,
		"removeComments": false,
		"sourceMap": true,
		"inlineSources": true,
		"noEmitOnError": true,
		"outDir": "./www/scripts"
	},
	"exclude": [ "platforms", "node_modules", "bower_components", "plugins", "merges", "res", "bin", "bld" ]
}